<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    
    <navigation-rule>
        <from-view-id>/UserRegister/UserConfirm.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>UserAcknowledge</from-outcome>
            <to-view-id>/acknowledge/UserAcknowledge.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
        
    <navigation-rule>
        <from-view-id>/DeederRegister/DeederConfirm.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>DeederAcknowledge</from-outcome>
            <to-view-id>/acknowledge/DeederAcknowledge.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/BusinessRegister/BusinessConfirm.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>BusinessAcknowledge</from-outcome>
            <to-view-id>/acknowledge/BusinessAcknowledge.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/GovernmentRegister/GovernmentConfirm.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>GovernmentAcknowledge</from-outcome>
            <to-view-id>/acknowledge/GovernmentAcknowledge.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/NgoRegister/NgoConfirm.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>NgoAcknowledge</from-outcome>
            <to-view-id>/acknowledge/NgoAcknowledge.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/UserDeederRegister/ConfirmDetails.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>UserDeederAcknowledge</from-outcome>
            <to-view-id>/acknowledge/UserDeederAcknowledge.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/CreateDeed/ConfirmDeed.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>DeedAcknowledge</from-outcome>
            <to-view-id>/acknowledge/DeedAcknowledge.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
        
    <navigation-rule>
        <from-view-id>/AccessConfirm.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>UserRegisterFinal</from-outcome>
            <to-view-id>/register/user/UserRegisterFinal.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>DeederRegisterFinal</from-outcome>
            <to-view-id>/register/deeder/DeederRegisterFinal.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>BusinessRegisterFinal</from-outcome>
            <to-view-id>/register/business/BusinessRegisterFinal.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>GovernmentRegisterFinal</from-outcome>
            <to-view-id>/register/government/GovernmentRegisterFinal.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>NgoRegisterFinal</from-outcome>
            <to-view-id>/register/ngo/NgoRegisterFinal.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
       
    <navigation-rule>
        <from-view-id>/UDAccessConfirm.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>DeederRegisterFinal</from-outcome>
            <to-view-id>/register/deeder/DeederRegisterFinal.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/Login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>UserHome</from-outcome>
            <to-view-id>/home/UserHome.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>DeederHome</from-outcome>
            <to-view-id>/home/DeederHome.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>BusinessHome</from-outcome>
            <to-view-id>/home/BusinessHome.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>GovernmentHome</from-outcome>
            <to-view-id>/home/GovernmentHome.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>NgoHome</from-outcome>
            <to-view-id>/home/NgoHome.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
</faces-config>
